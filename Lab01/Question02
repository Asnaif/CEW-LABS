#include<stdio.h>
int main(){
    float width;
    float height;
    printf("Enter width of Rectangle: \n");
    scanf("%f", &width);

    printf("Enter height of Rectangle: \n");
    scanf("%f", &height);

    float area = width*height;
    float perimeter = 2*(width+height);

    printf("your area is: %f\ngcc", area);
    printf("your perimeter  is: %f", perimeter);
    return 0;
}
